<SWBox>:
    orientation:'vertical'
    radius:dp(7),dp(7),dp(7),dp(7)
    md_bg_color:1,0,0,1
    adaptive_height:True
    padding:dp(4)
    MDBoxLayout:
        size_hint_y:None
        height:dp(35)
        MDLabel:
            text:root.text
            font_style:'Overline'
            font_size:dp(22)
            halign:'center'
            bold:True
            color:root.text_color
    