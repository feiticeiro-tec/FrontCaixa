<SWTableWithTitle>:
    orientation:'vertical'
    adaptive_width:True
    spacing:dp(5)
    pos_hint:{'top':1}
    MDBoxLayout:
        orientation:'vertical'
        radius:dp(7),dp(7),dp(7),dp(7)
        md_bg_color:root.color
        adaptive_width:True
        padding:dp(4)
        MDBoxLayout:
            size_hint_y:None
            height:dp(35)
            MDLabel:
                text:'Produtos Adicionados'
                font_style:'Overline'
                font_size:dp(22)
                halign:'center'
                bold:True
                color:root.text_color
        MDBoxLayout:
            id:table
            size_hint_x:None
            md_bg_color:1,1,1,1
            width:dp(690)